#include<stdio.h> 
#include<conio.h>
 void main() 
{
 int a[50], i, n, max, min; 
printf(“Enter size of the array: \n”); 
scanf(“%d”, &n); 
printf(“Enter elements in the array: \n”); 
for(i = 0;i < n;i++) 
{ 
scanf(“%d”, &a[i]); 
}
 max=a[0]; 
min=a[0]; 
for(i = 1;i < n;i++) 
{
 if(a[i] > max) 
max=a[i]; 
if(a[i] < min) 
min=a[i]; 
}
 printf(“Maximum element = %d \n”, max); 
printf(“Minimum element = %d \n”, min); 
}
